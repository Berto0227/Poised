     projectDetails += &quot;\nArchitect's Name:\t\t\t\t &quot; + mainProject.get(8);&#xD;
     projectDetails += &quot;\nArchitect's Telephone Number:\t &quot; + mainProject.get(9);&#xD;
     projectDetails += &quot;\nArchitect's Email Address:\t\t &quot; + mainProject.get(10);&#xD;
